[32m+[m[32m// ⌨️ Typewriter Hook[m
[32m+[m[32mfunction useTypewriter(text: string, speed = 30, delay = 0) {[m
[32m+[m[32m    const [displayed, setDisplayed] = useState("");[m
[32m+[m[32m    useEffect(() => {[m
[32m+[m[32m        setDisplayed("");[m
[32m+[m[32m        const timeout = setTimeout(() => {[m
[32m+[m[32m            const interval = setInterval(() => {[m
[32m+[m[32m                setDisplayed((prev) => {[m
[32m+[m[32m                    if (prev.length < text.length) {[m
[32m+[m[32m                        return prev + text[prev.length];[m
[32m+[m[32m                    } else {[m
[32m+[m[32m                        clearInterval(interval);[m
[32m+[m[32m                        return prev;[m
[32m+[m[32m                    }[m
[32m+[m[32m                });[m
[32m+[m[32m            }, speed);[m
[32m+[m[32m        }, delay);[m
[32m+[m[32m        return () => clearTimeout(timeout);[m
[32m+[m[32m    }, [text, speed, delay]);[m
[32m+[m[32m    return displayed;[m
[32m+[m[32m}[m
